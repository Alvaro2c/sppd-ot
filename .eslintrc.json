{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-undef": "error",
    "no-unreachable": "error",
    "no-constant-condition": "warn",
    "no-dupe-keys": "error",
    "no-dupe-args": "error",
    "no-dupe-case": "error",
    "no-dupe-else-if": "error",
    "no-empty": "warn",
    "no-extra-semi": "error",
    "no-irregular-whitespace": "error",
    "no-unexpected-multiline": "error",
    "valid-typeof": "error"
  }
} 